{
  "name": "apollo-react-architecture",
  "version": "0.1.0",
  "private": true,
  "main": "build/Routes.js",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^4.4.3",
    "apollo-boost": "^0.1.16",
    "apollo-link-ws": "^1.0.9",
    "graphql": "^14.0.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.0",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf build",
    "build:ibrp": "npm run prebuild && babel src -d build --copy-files",
    "build": "react-scripts build",
    "postinstall": "npm run build:ibrp",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --runInBand --coverage",
    "test:c": "jest --coverage",
    "lint-new": "eslint ./src/** --ext .js --ext .jsx",
    "lint": "eslint ./src/** --ext .js --ext .jsx",
    "lint-fix": "eslint ./src/** --ext .js --ext .jsx --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^2.1.0"
  }
}
